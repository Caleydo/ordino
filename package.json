{
  "name": "targid2",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/targid2",
  "dependencies": {
  },
  "peerDependencies": {
    "clue": "*",
    "caleydo_vis_lineup": "*"
  },
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "application",
          "name": "Targid V2"
        },
        {
          "type": "actionFactory",
          "file": "cmds",
          "factory": "createCmd",
          "creates": "(showTargetArea|hideTargetArea|changeRatioTargetArea|selectTargidData)"
        }
      ]
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.5",
        "lineupjs": "https://github.com/Caleydo/lineup.js.git"
      }
    },
    "products": [
      {
        "name": "Targid V2",
        "type": "python",
        "app": "targid2",
        "plugins": [
          "targid2",
          "caleydo_server"
        ]
      }
    ]
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
